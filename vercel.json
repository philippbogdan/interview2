{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/health",
      "dest": "api/index.py"
    },
    {
      "src": "/init",
      "dest": "api/index.py"
    },
    {
      "src": "/generate/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/variables/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/coverage/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/session/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}
